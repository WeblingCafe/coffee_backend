{
  "family": "cf-backend-task-definition",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/cf-nginx",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "name": "coffee-backend-nginx",
      "image": "012152056735.dkr.ecr.ap-northeast-2.amazonaws.com/cf-nginx:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        },
        {
          "containerPort": 443,
          "hostPort": 443,
          "protocol": "tcp"
        }
      ],
      "dependsOn": [
        {
          "containerName": "coffee-backend",
          "condition": "START"
        }
      ],
      "essential": true
    },
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/cf-api",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "name": "coffee-backend",
      "image": "012152056735.dkr.ecr.ap-northeast-2.amazonaws.com/cf-api:latest",
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "command": [
        "java",
        "-Dspring.config.activate.on-profile=prd",
        "-Duser.timezone=Asia/Seoul",
        "-javaagent:/pinpoint-agent-2.5.1/pinpoint-bootstrap-2.5.1.jar",
        "-Dpinpoint.agentId=cf",
        "-Dpinpoint.applicationName=webling-cf-prd",
        "-DCOLLECTOR_IP=10.0.0.181",
        "-DPROFILER_TRANSPORT_AGENT_COLLECTOR_PORT=9991",
        "-DPROFILER_TRANSPORT_METADATA_COLLECTOR_PORT=9991",
        "-DPROFILER_TRANSPORT_STAT_COLLECTOR_PORT=9992",
        "-DPROFILER_TRANSPORT_SPAN_COLLECTOR_PORT=9993",
        "-DPROFILER_SAMPLING_TYPE=COUNTING",
        "-DPROFILER_SAMPLING_COUNTING_SAMPLING_RATE=1",
        "-DPROFILER_SAMPLING_PERCENT_SAMPLING_RATE=100",
        "-DPROFILER_SAMPLING_NEW_THROUGHPUT=0",
        "-DPROFILER_SAMPLING_CONTINUE_THROUGHPUT=0",
        "-DDEBUG_LEVEL=INFO",
        "-DPROFILER_TRANSPORT_MODULE=GRPC",
        "-jar",
        "/usr/local/coffee.jar"
      ],
      "environment": [
        {
          "name": "SPRING_CONFIG_ACTIVATE_ON_PROFILE",
          "value": "prd"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "essential": true
    }
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "2048",
  "memory": "4096",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ]
}